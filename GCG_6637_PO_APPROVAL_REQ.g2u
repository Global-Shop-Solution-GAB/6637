F.Intrinsic.Control.If(V.Local.bExist,=,False)
	F.Intrinsic.String.Concat(V.Local.sFileRW,V.Ambient.NewLine,V.Local.sPOLine.Trim,"*!*",0,"*!*",V.Local.fAmtS,V.Local.sFileRW)
